<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="agrt.dbs.assestsmanagement" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="app.niit.hackaton.agrt.ui.LoginActivity"
            android:screenOrientation="sensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="app.niit.hackaton.agrt.ui.DashboardActivity"
            android:screenOrientation="sensor" />

        <provider
            android:name="app.niit.hackaton.agrt.provider.AgtrProvider"
            android:authorities="hack.agtr.provider"
            android:exported="false" />

    </application>

</manifest>
